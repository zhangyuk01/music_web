<h1>歌曲列表</h1>
<table>
  <thead>
    <tr>
      <th>姓名:</th>
      <th>性别:</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @musics.each do |music| %>
      <tr>
        <td><%= music.song_name %></td>
        <td><%= music.type %></td>
        <td><%= link_to '显示', music %></td>
        <td><%= link_to '编辑', edit_music_path(music) %></td>
        <td><%= link_to '删除', music, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
      <audio controls="controls">
        <source src="平凡之路.mp3" type="audio/mpeg">
      </audio>
      <br/>

      <audio controls="controls">
        <source src="唐人.mp3" type="audio/mpeg">
      </audio>
      <br/>
      <audio controls="controls">
        <source src=" 最简单的声音.mp3" type="audio/mpeg">
      </audio>
  </tbody>
</table>
<br>

<%= link_to '添加歌曲', new_music_path %>
